[MASTER]
init-hook="import sys; sys.path.append('.')"

[BASIC]
good-names=i, j, k, y
good-names-rgxs=x[0-9]?$

[TYPECHECK]
generated-members=torch.*

[pylint.FORMAT]
disable=logging-fstring-interpolation

[MESSAGES CONTROL]
disable=
    too-few-public-methods,

[DESIGN]
max-args=10
max-locals=25
max-attributes=11
